
class NewTest
{
    function NewTest()
    {
        System.log("NewTest: constructor!");
    }

    function method1(arg1)
    {
        System.log("NewTest.method1: arg1=" + arg1);
    }

    function main() static
    {
        var obj = new NewTest();
        obj.method1("Hello");
        obj.method1("World");
    }
}

